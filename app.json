{
  "expo": {
    "name": "Portion Track",
    "slug": "Portion Track",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/c120b509-3f86-4f37-80ee-1220bafc3458.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/a8b6ca2d-aad0-43bb-b5fe-a3c6bd7e7585.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.portiontrack.app",
      "icon": "./assets/images/c120b509-3f86-4f37-80ee-1220bafc3458.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/c120b509-3f86-4f37-80ee-1220bafc3458.png",
        "backgroundColor": "#FFFFFF",
        "monochromeImage": "./assets/images/c120b509-3f86-4f37-80ee-1220bafc3458.png"
      },
      "package": "com.portiontrack.app",
      "icon": "./assets/images/c120b509-3f86-4f37-80ee-1220bafc3458.png"
    },
    "web": {
      "favicon": "./assets/images/c120b509-3f86-4f37-80ee-1220bafc3458.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "portion-track",
    "experiments": {
      "typedRoutes": true
    }
  },
  "scheme": "Portion Track"
}