
{
  "expo": {
    "name": "Portion Track",
    "slug": "portion-track",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/efcc7b9b-45a9-47be-b8ad-f8e8ccb016ce.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/a8b6ca2d-aad0-43bb-b5fe-a3c6bd7e7585.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.portiontrack.app",
      "icon": "./assets/images/efcc7b9b-45a9-47be-b8ad-f8e8ccb016ce.png",
      "buildNumber": "6",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/efcc7b9b-45a9-47be-b8ad-f8e8ccb016ce.png",
        "backgroundColor": "#FFFFFF",
        "monochromeImage": "./assets/images/efcc7b9b-45a9-47be-b8ad-f8e8ccb016ce.png"
      },
      "package": "com.portiontrack.app",
      "versionCode": 4,
      "icon": "./assets/images/efcc7b9b-45a9-47be-b8ad-f8e8ccb016ce.png",
      "permissions": [
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/images/efcc7b9b-45a9-47be-b8ad-f8e8ccb016ce.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/efcc7b9b-45a9-47be-b8ad-f8e8ccb016ce.png",
          "color": "#B93822",
          "defaultChannel": "daily-reminders"
        }
      ]
    ],
    "scheme": "portiontrack",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
