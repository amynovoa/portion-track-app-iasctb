{
  "expo": {
    "name": "Portion Track",
    "slug": "Portion Track",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/portiontracker_icon_1024.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/portiontracker_splash_2048.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.portiontrack.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/portiontracker_icon_1024.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.portiontrack.app"
    },
    "web": {
      "favicon": "./assets/images/portiontracker_icon_1024.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "portiontrack",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  },
  "scheme": "Portion Track"
}