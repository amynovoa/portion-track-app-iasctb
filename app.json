
{
  "expo": {
    "name": "Portion Track",
    "slug": "Portion Track",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/5cef0cc8-0b86-4da4-86ba-244665f65041.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/a8b6ca2d-aad0-43bb-b5fe-a3c6bd7e7585.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.portiontrack.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/5cef0cc8-0b86-4da4-86ba-244665f65041.png",
        "backgroundColor": "#FFFFFF",
        "monochromeImage": "./assets/images/5cef0cc8-0b86-4da4-86ba-244665f65041.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.portiontrack.app"
    },
    "web": {
      "favicon": "./assets/images/5cef0cc8-0b86-4da4-86ba-244665f65041.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "portiontrack",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  },
  "scheme": "Portion Track"
}
